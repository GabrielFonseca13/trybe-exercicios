      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^9.7.0",
    "cypress-multi-reporters": "^1.4.0",
    "eslint-config-trybe-frontend": "1.3.0",
    "fs-extra": "^9.0.1",
    "mocha": "^8.0.1",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "stylelint": "^14.9.1",
